{
  "name": "lrnapp-gitbook",
  "description": "",
  "start_url": "/",
  "display": "standalone",
  "app_integration": {
    "title": "Gitbook",
    "endpoints": {
      "api/books": {
        "callback": "_lrnapp_gitbook_books_callback",
        "property": "source-path"
      },
      "api/books/%": {
        "callback": "_lrnapp_gitbook_book_callback",
        "property": "source-path"
      },
      "api/books/%/canmerge": {
        "callback": "_lrnapp_gitbook_canmerge_callback",
        "property": "source-path"
      },
      "api/books/%/sync": {
        "callback": "_lrnapp_gitbook_sync_callback",
        "property": "source-path"
      }
    }
  }
}